# Janus SIP Plugin Configuration
# For 3CX Translation Bridge on Linux VM

general: {
    local_ip = "10.0.5.19"
    sdp_ip = "10.0.5.19"
    rtp_port_range = "10000-10100"
    user_agent = "3CX-Translation-Bridge/1.0"
